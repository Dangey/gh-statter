{
    "repos": [
        "microsoft/vscode",
        "xamarin/Xamarin.Forms",
        "actions/runner"
    ],
    "routes": {
        "repo": {
            "path": "GET /repos/{owner}/{repo}",
            "fields": {
                "avatar": [
                    "owner",
                    "avatar_url"
                ],
                "forks": "forks_count",
                "stars": "stargazers_count",
                "watchers": "watchers_count",
                "open_issues": "open_issues_count"
            }
        }
    }
} 